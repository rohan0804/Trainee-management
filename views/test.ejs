<div class="loader-wrapper">
  <div class="preloader" id="loader"></div>
</div>
<div class="container" style="margin-top: 16px;">
  <h1 class="page-title">Assign a test:</h1>
  <hr />
  <div class="add-container wow fadeInUp">
    <form action="/mentor/addtest" method="POST" enctype="multipart/form-data">
      <div class="form-group">
        <label for="name">Name</label>
        <input
          type="text"
          class="form-control"
          id="name"
          name="name"
          required
        />
      </div>
      <div class="form-group">
        <label for="date">Date</label>
        <input
          type="text"
          name="date"
          id="datepicker"
          class="form-control datepicker"
          placeholder="YYYY/MM/DD"
          data-date-format="yyyy-mm-dd"
          autocomplete="off"
          required
        />
      </div>
      <div class="form-group">
        <label for="description">Questions File</label>
        <input
          type="file"
          name="testfile"
          id="file"
          class="form-control"
          placeholder="Choose file"
          required
        />
      </div>
      <div class="form-group">
        <label for="duration">Duration</label>
        <input
          type="text"
          class="form-control"
          name="duration"
          id="duration"
          placeholder="00:00:00"
          required
        />
      </div>
      <div class="form-group">
        <label for="totalmarks">Total Marks</label>
        <input
          type="text"
          class="form-control"
          name="totalmarks"
          id="totalmarks"
          required
        />
      </div>
      <input type="hidden" name="mentor_id" value="1" />
      <button type="submit" class="btn buttn" id="addtestbtn">
        Add Test
      </button>
    </form>
  </div>
</div>
<script src="/js/wow.js"></script>
<script src="/js/jquery.min.js"></script>
<script src="/js/jquery-ui.js"></script>
<script src="/js/bootstrap.min.js"></script>
<script src="/js/jquery-1.12.4.js"></script>
<script src="/js/bootstrap-datepicker.min.js"></script>
<script src="/js/sidebar.js"></script>
<script src="https://unpkg.com/notie"></script>
<script>
  $(function () {
    $("#datepicker").datepicker({ dateFormat: "yy-mm-dd" });
  });
  if ("<%=status%>" === "true") {
    notie.alert({
      position: "bottom",
      time: 3,
      type: "success",
      text: "<h2>Test Added Successfully</h2>",
    });
  }
  console.log(typeof "<%=status%>");
</script>
